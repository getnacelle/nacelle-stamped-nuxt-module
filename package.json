{
  "name": "@nacelle/nacelle-stamped-nuxt-module",
  "version": "0.0.8",
  "description": "Integrates Stamped.io into your Nacelle Nuxt project",
  "license": "MIT",
  "contributors": [
    {
      "name": "Nacelle"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/ShopElf/nacelle-stamped-nuxt-module",
  "scripts": {
    "dev": "nuxt example",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./lib",
    "test": "jest ./lib",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.11.0",
    "@types/jest": "^26.0.8",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.2.2",
    "eslint": "^7.5.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  },
  "lint-staged": {
    "src/**/*.(js|ts)": [
      "prettier --write",
      "npm run lint"
    ],
    "*{json, md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  }
}
